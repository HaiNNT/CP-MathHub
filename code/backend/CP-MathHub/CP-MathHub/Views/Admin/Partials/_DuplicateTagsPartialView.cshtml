@using CP_MathHub.Models.Admin
@model ICollection<TagViewModel>
@for (int i = 0; i < Model.Count; i++)
{
    TagViewModel tag = Model.ElementAt(i);
        <tr class="">
            <td class="mh-cell-center">@(i + 1)</td>
            <td class="mh-cell-center">@tag.Name</td>
            <td class="mh-cell-middle">@tag.Description</td>
            <td class="mh-cell-center"><input class="checkduplicate" type="checkbox" value="@tag.Id" checked></td>
        </tr>
}

